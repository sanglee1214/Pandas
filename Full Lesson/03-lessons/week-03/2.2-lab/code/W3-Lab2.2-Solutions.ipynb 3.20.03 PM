    "# Here's some quadratic data with a bit of noise\n",
    "import scipy\n",
    "\n",
    "noise = scipy.stats.norm(0, 4)\n",
    "data = [(x, 3*x*x - 5*x + 3 + noise.rvs()) for x in np.arange(-6, 2, 0.5)]\n",
    "print data\n",
    "xs = [x for (x, y) in data]\n",
    "ys = [y for (x, y) in data]\n",
    "ys"
   ]
